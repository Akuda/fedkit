{
  "name": "fedkit",
  "version": "0.4.0-alpha.1",
  "homepage": "https://github.com/Akuda/fedkit",
  "authors": [
    "Phil Lennon <enuiry@akuda.co.uk>"
  ],
  "moduleType": [
    "globals"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "susy": "2.2.3",
    "jquery": "1",
    "modernizr": "~2.8.3",
    "sass-mq": "~3.1.2",
    "normalize-scss": "~3.0.3"
  },
  "install": {
    "base": "src/assets",
    "path": {
      "js": "js/vendor/{name}",
      "scss": "scss/vendor/{name}",
      "css": "scss/vendor/{name}",
      "eot": "fonts/vendor/{name}",
      "svg": "fonts/vendor/{name}",
      "ttf": "fonts/vendor/{name}",
      "woff": "fonts/vendor/{name}"
    },
    "sources": {
      "modernizr": "bower_components/modernizr/modernizr.js",
      "susy": "bower_components/susy/sass/**"
    }
  }
}
